  "scripts": {
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build",
    "dev": "webpack serve --open --mode development",
    "build": "webpack --mode production",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "lint": "biome lint .",
    "typecheck": "tsc --noEmit",
    "serve": "serve dist -p 3001 -s",
    "clean": "rm -rf dist",
    "test": "craco test",
    "render": "ts-node src/ssg.ts",
    "build-icons": "rm -rf ./src/icon/generated && pnpm run generate-icons",
    "generate-icons": "svgr --template ./src/icon/template.js --typescript --ext tsx --icon --replace-attr-values '#FFF=currentColor,#181E27=currentColor' --out-dir=./src/icon/generated ./static/icons"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
