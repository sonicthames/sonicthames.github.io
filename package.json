{
  "name": "sonicthames.github.io",
  "homepage": "https://sonicthames.github.io",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "@svgr/cli": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "d3-ease": "^3.0.1",
    "fp-ts": "^2.16.11",
    "gh-pages": "^3.2.3",
    "history": "^4.10.1",
    "io-ts": "^2.2.22",
    "luxon": "^2.2.0",
    "mapbox-gl": "^2.15.0",
    "material-ui": "^0.20.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-map-gl": "^6.1.18",
    "react-responsive": "^9.0.0-beta.5",
    "react-router": "^5.3.0",
    "react-router-dom": "^5.3.0",
    "rxjs": "^7.8.2",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@types/d3-ease": "^3.0.2",
    "@types/history": "^4.7.11",
    "@types/luxon": "^2.0.8",
    "@types/mapbox-gl": "^2.7.20",
    "@types/node": "^24.7.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.4",
    "jsdom": "^27.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "deploy": "tsx scripts/deploy.ts",
    "dev": "vite --open --port 3001",
    "build": "vite build",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "lint": "biome lint .",
    "typecheck": "tsc --noEmit",
    "serve": "vite preview --port 3001 --open",
    "clean": "rm -rf dist",
    "test": "vitest",
    "build-icons": "rm -rf ./src/icon/generated && pnpm run generate-icons",
    "generate-icons": "svgr --template ./src/icon/template.js --typescript --ext tsx --icon --expand-props none --replace-attr-values '#FFF=currentColor,#181E27=currentColor' --out-dir=./src/icon/generated ./static/icons"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
